 <Link to='/'>Go Back</Link>\r\n          </Message>\r\n        ) : (\r\n          <ListGroup variant='flush'>\r\n            {cartItems.map((item) => (\r\n              <ListGroup.Item key={item.product}>\r\n                <Row>\r\n                  <Col md={2}>\r\n                    <Image src={item.image} alt={item.name} fluid rounded />\r\n                  </Col>\r\n                  <Col md={3}>\r\n                    <Link to={`/product/${item.product}`}>{item.name}</Link>\r\n                  </Col>\r\n                  <Col md={2}>{item.price}</Col>\r\n                  <Col md={2}>\r\n                    <Form.Control\r\n                      as='select'\r\n                      value={item.qty}\r\n                      onChange={(e) =>\r\n                        dispatch(\r\n                          addToCart(item.product, Number(e.target.value))\r\n                        )\r\n                      }\r\n                    >\r\n                      {[...Array(item.countInStock).keys()].map((x) => (\r\n                        <option key={x + 1} value={x + 1}>\r\n                          {x + 1}\r\n                        </option>\r\n                      ))}\r\n                    </Form.Control>\r\n                  </Col>\r\n                  <Col md={2}>\r\n                    <Button\r\n                      type='button'\r\n                      variant='light'\r\n                      onClick={() => removeFromCartHandler(item.product)}\r\n                    >\r\n                      <i className='fas fa-trash'></i>\r\n                    </Button>\r\n                  </Col>\r\n                </Row>\r\n              </ListGroup.Item>\r\n            ))}\r\n          </ListGroup>\r\n        )}\r\n      </Col>\r\n      <Col md={4}>\r\n        <Card>\r\n          <ListGroup variant='flush'>\r\n            <ListGroup.Item>\r\n              <h2>\r\n                Subtotal ({cartItems.reduce((acc, item) => acc + item.qty, 0)})\r\n                items\r\n              </h2>\r\n              $\r\n              {cartItems\r\n                .reduce((acc, item) => acc + item.qty * item.price, 0)\r\n                .toFixed(2)}\r\n            </ListGroup.Item>\r\n            <ListGroup.Item>\r\n              <Button\r\n                type='button'\r\n                className='btn-block'\r\n                disabled={cartItems.length === 0}\r\n                onClick={checkoutHandler}\r\n              >\r\n                Proceed To Checkout\r\n              </Button>\r\n            </ListGroup.Item>\r\n          </ListGroup>\r\n        </Card>\r\n      </Col>\r\n    </Row>\r\n  )\r\n}\r\n\r\nexport default CartScreen\r\n",["139","140"],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\actions\\cartActions.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\constants\\userConstants.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\components\\FormContainer.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\screens\\LoginScreen.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\actions\\userActions.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\reducers\\userReducers.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\screens\\RegisterScreen.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\screens\\ProfileScreen.js",["141"],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\screens\\ShippingScreen.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\constants\\cartConstants.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Projects\\proshop\\frontend\\src\\components\\CheckoutSteps.js",[],"C:\\Users\\iamkg\\Desktop\\kg\\Study\\Reactjs\\Project